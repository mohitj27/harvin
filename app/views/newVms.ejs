<meta charset="UTF-8">
<title>Harvin Academy</title>
<script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
<meta name="viewport" content="width=device-width">


<!-- Compiled and minified CSS -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">

<!-- Compiled and minified JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>

<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<script src="/dist/DateTimePicker.min.js" type="text/javascript"></script>
<script src="/js/newVms.js" type="text/javascript"></script>
<link href="/css/newVms.css" type="text/css" rel="stylesheet"></link>
<link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">

<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">


<body>
	<nav class="no-pad z-depth-0">
			<h2>HARVIN ACADEMY</h2>
	</nav>
	<main>
		<div class="section no-pad">

			<div class="container">

				<form action="/vms" method="post">
					<div class="row input-field">
						<input id="name" name="name" type="text" required autofocus />
						<label for="name">
								<span class="label-text">Full Name</span>
								<span class="nav-dot"></span>
							</label>
					</div>
					<div class="row input-field">
						<select id="classs" type="text" name="classs" data-alignment="right">
							<option value="" disabled selected>Choose your option</option>
								 <option value="IX">IX</option>
								 <option value="X">X</option>
								 <option value="XI">XI</option>
								 <option value="XII">XII</option>
								 <option value="XII-Pass">XII-Pass</option>
							 </select>
						<label for="classs">Class</label>
					</div>
					<div class="row input-field">
						<input id="emailId" name="emailId" type="email" required />
						<label for="emailId">
								<span class="label-text">Email</span>
								<span class="nav-dot"></span>
							</label>
					</div>
					<div class="row input-field">
						<input id="phone" name="phone" type="tel" minlength="10" maxlength="10" required />
						<label for="phone">
								<span class="label-text">Phone</span>
								<span class="nav-dot"></span>
							</label>
					</div>




<div class="row center">

						<button class="btn">Submit</button>

</div>

				</form>
				<!-- <footer>
						<a href="#"><i class="material-icons">keyboard_arrow_left</i></a>
						<p id="counter">1/6</p>
						<a href="#"><i class="material-icons">keyboard_arrow_right</i></a>
					</footer> -->
			</div>
		</div>

		</div>

	</main>
</body>

</html>
