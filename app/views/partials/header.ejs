<body>
	<header>

		<a href="#" data-activates="slide-out" class="button-collapse"><i id="menu-button" class="material-icons">menu</i></a>
		<!--  -->
		<ul id="slide-out" class="side-nav z-depth-3 gradient-45deg-green-teal">
			<li><a href="#!">First Sidebar Link</a></li>
			<li><a href="#!">Second Sidebar Link</a></li>
			<li class="no-padding">
				<ul class="collapsible collapsible-accordion">
					<li>
						<a class="collapsible-header">Insert in DB<i class="material-icons">arrow_drop_down</i></a>
						<div class="collapsible-body">
							<ul>
								<li><a href="/admin/files/uploadFile">Upload File</a></li>
								<li><a href="/admin/batches/updateBatch">Create Batch</a></li>
								<li><a href="/admin/exams/new">Set an Exam</a></li>
								<li><a href="/admin/questionBank">Question Bank</a></li>
								<li><a href="/admin/assignment/uploadAssignment">New Assignment</a></li>
								<li><a href="/admin/batches/updateBatch">Create Batch</a></li>
								<li><a href="/admin/blog">New Post</a></li>
								<li><a href="/admin/db/gallery/upload">Upload Image</a></li>
							</ul>
						</div>
					</li>
				</ul>
			</li>
			<li class="no-padding">
				<ul class="collapsible collapsible-accordion">
					<li>
						<a class="collapsible-header">Blog<i class="material-icons">arrow_drop_down</i></a>
						<div class="collapsible-body">
							<ul>
								<li><a href="/admin/blog">New Post</a></li>
								<li><a href="/admin/db/gallery/upload">All Blogs</a></li>
							</ul>
						</div>
					</li>
				</ul>
			</li>
			<% if(!currentUser){ %>

				<li>
					<a href="/admin/login">Login</a>
				</li>
				<li>
					<a href="/admin/signup">Signup</a>
				</li>
				<% }else{ %>
					<li>
						<a href="#">Signed in as
											<%= currentUser.username %>
										</a>
					</li>
					<li>
						<a href="/admin/logout">Logout</a>
					</li>
					<% } %>
		</ul>
	</header>
	<main>
		<div class="container center-align">
			<% if(msg_error && msg_error.length > 0){ %>
				<div class="card-panel orange darken-3 black-text">
					<%=msg_error%>
				</div>
				<% } %>

					<% if(msg_success && msg_success.length > 0){ %>

						<div class="card-panel light-green black-text">
							<%=msg_success%>
						</div>

						<% } %>
		</div>
	</main>
	<script src="/script/main.js"></script>
