<% include partials/header.ejs%>

  <main>
    <div class="container">
      <div class="center-align">
        <h3> Create New Subject </h1>
        </div>
          <hr>
          <br>
          <br>
        <form id="uploadForm" enctype="multipart/form-data" method="post" action="/admin/subjects">
          <div class="row">
            <div class="col s4 offset-s2">
              <div class="input-field ">
                <input name="className" required type="text" id="className" autocomplete="off" class="autocomplete validate">
                <label for="className">Class Name</label>
              </div>
            </div>

            <div class="col s4">
              <div class="input-field ">
                <input name="subjectName" required type="text" id="subjectName" autocomplete="off" class="autocomplete validate">
                <label for="subjectName">Subject Name</label>
              </div>
            </div>
          </div>

          <div class="center-align">
            <input type="submit" name="submit" class="btn" />
          </div>
        </form>
      </div>
    </main>
  <script src="/js/uploadFile.js" charset="utf-8"></script>
    <% include partials/footer.ejs%>
